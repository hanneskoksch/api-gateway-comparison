{
  "name": "Echo Service",
  "use_keyless": true,
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default"
      }
    }
  },
  "proxy": {
    "listen_path": "/",
    "enable_load_balancing": true,
    "target_list": [
      "http://echo-a:5001",
      "http://echo-b:5001",
      "http://echo-c:5001"
    ],

    "strip_listen_path": true
  }
}
